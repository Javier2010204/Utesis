<p id="notice"><%= notice %></p>

<h1>Theses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Career</th>
      <th>Grade</th>
      <th>University</th>
      <th>Delivery time</th>
      <th>Type document</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @theses.each do |thesis| %>
      <tr>
        <td><%= thesis.name %></td>
        <td><%= thesis.phone %></td>
        <td><%= thesis.email %></td>
        <td><%= thesis.career %></td>
        <td><%= thesis.grade %></td>
        <td><%= thesis.university %></td>
        <td><%= thesis.delivery_time %></td>
        <td><%= thesis.type_document %></td>
        <td><%= thesis.body %></td>
        <td><%= link_to 'Show', thesis %></td>
        <td><%= link_to 'Edit', edit_thesis_path(thesis) %></td>
        <td><%= link_to 'Destroy', thesis, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Thesis', new_thesis_path %>
